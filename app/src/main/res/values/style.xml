<resources xmlns:tools="http://schemas.android.com/tools">
	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
		<item name="colorPrimary">@color/white</item>
		<item name="android:statusBarColor" tools:targetApi="l">@color/teal_200</item>
		<item name="colorPrimaryDark">@android:color/white</item>
	
	
	</style>
	
	<style name="ShapeAppearanceOverlay.App.CornerSize6Percent" parent="">
		<item name="cornerSize">2%</item>
	</style>
	
	
	<style name="text_header_700wt_20sp">
		<item name="android:textSize">15sp</item>
		<item name="android:fontFamily">@font/roboto_medium</item>
		<item name="android:textColor">@color/green</item>
		<item name="android:fontWeight" tools:targetApi="o">700</item>
	</style>
	
	
	
	<style name="text_header_bold_20sp">
		<item name="android:textSize">15sp</item>
		<item name="android:fontFamily">@font/roboto_medium</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:fontWeight" tools:targetApi="o">700</item>
	</style>
	
	<style name="text_header_bold_15sp_white">
		<item name="android:textSize">15sp</item>
		<item name="android:fontFamily">@font/roboto_medium</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:fontWeight" tools:targetApi="o">700</item>
	</style>
	
	
	<style name="text_header_bold_15sp_black">
		<item name="android:textSize">18sp</item>
		<item name="android:fontFamily">@font/roboto_bold</item>
		<item name="android:textColor">@color/black_gray</item>
		<item name="android:fontWeight" tools:targetApi="o">700</item>
	</style>
	
	
	<style name="text_header_bold_15sp_gray">
		<item name="android:textSize">15sp</item>
		<item name="android:fontFamily">@font/roboto_bold</item>
		<item name="android:textColor">@color/black_gray</item>
		<item name="android:fontWeight" tools:targetApi="o">700</item>
	</style>
	
	
	
</resources>